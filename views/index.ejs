<!-- include header -->
<%- include('include/_header')%>
<!-- /include header -->



<!-- main site -->
<main id="site-main">
    <div class="container">
        <div class="box-nav d-flex justify-between">
            <a href="/add-user" class="border-shadow">
                <span>New User</span>
            </a>
        </div>
    </div>
</main>

<!-- form handling -->
<div class="container">
<form action="/" method="post">
    <table class="table">
        <thead class="thead-dark">
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>@Email</th>
                <th>Gender</th>
                <th>Status</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            <%- include('include/_show')%>
        </tbody>
    </table>
</form>
</div>
<!-- /form handling -->

<!-- /main site -->



<!-- include footer -->
<%- include('include/_footer')%>
<!-- /include footer -->